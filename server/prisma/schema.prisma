datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model File {
  id           String   @id @default(uuid())
  originalName String
  fileName     String
  fileType     String
  filePath     String
  nodeId       String
  inputId      String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt

  @@index([nodeId])
  @@index([inputId])
} 
